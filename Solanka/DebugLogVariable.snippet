<?xml version="1.0" encoding="utf-8"?>
<CodeSnippets xmlns="http://schemas.microsoft.com/VisualStudio/2005/CodeSnippet">
	<CodeSnippet Format="1.0.0">
		<Header>
			<Title>Debug.Log(Variable)</Title>
			<Author>Dreamer</Author>
			<Description>Inserts Debug.Log(Variable);</Description>
			<Shortcut>dv</Shortcut>
		</Header>    
		<Snippet>
			<Code Language="CSharp"><![CDATA[Debug.Log($str$);$end$]]></Code>
			<Declarations>
				<Literal>
					<ID>str</ID>
					<ToolTip>Replace with a message.</ToolTip>
					<Default>Variable</Default>
				</Literal>
			</Declarations>
		</Snippet>
	</CodeSnippet>
</CodeSnippets>