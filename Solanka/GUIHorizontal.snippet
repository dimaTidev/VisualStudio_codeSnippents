<?xml version="1.0" encoding="utf-8"?>
<CodeSnippets xmlns="http://schemas.microsoft.com/VisualStudio/2005/CodeSnippet">
	<CodeSnippet Format="1.0.0">
		<Header>
			<Title>GUILayout.BeginHorizontal</Title>
			<Author>DimaTi</Author>
			<Description>Insert GUILayout.BeginHorizontal</Description>
			<Shortcut>guihor</Shortcut>
		</Header>    
		<Snippet>
			<Code Language="CSharp">
        <![CDATA[
        GUILayout.BeginHorizontal("$name$");

				GUILayout.EndHorizontal();
      ]]></Code>
			<Declarations>
				<Literal>
					<ID>name</ID>
					<ToolTip>Replace with name.</ToolTip>
					<Default>Helpbox</Default>
				</Literal>
			</Declarations>
		</Snippet>
	</CodeSnippet>
</CodeSnippets>